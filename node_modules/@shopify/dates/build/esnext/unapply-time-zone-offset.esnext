import { getTimeZoneOffset } from './get-time-zone-offset.esnext';

function unapplyTimeZoneOffset(date, timeZone1, timeZone2) {
  const timeZoneOffset = getTimeZoneOffset(date, timeZone1, timeZone2);
  return new Date(date.valueOf() + timeZoneOffset * 60 * 1000);
}

export { unapplyTimeZoneOffset };
